package javaapplication148;


public class ArrayIndex
{
    public static void main(String[] args) 
    {
        final int SIZE = 20;
        
        int [] array = new int [SIZE];
        
        for(int i = 0; i < SIZE; i++)
        {
            array[i] = i * 5;
        }
        
        for(int z : array)
        {
            System.out.println(z);
        }
    }
}
