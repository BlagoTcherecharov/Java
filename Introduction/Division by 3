package javaapplication135;
import java.util.Scanner;
import java.text.DecimalFormat;

public class JavaApplication135 
{

    public static void main(String[] args) 
    {
    
       for(int i = 1; i <= 100; i++)
       {
           if(i % 3 == 0)
           {
               System.out.println(i);
           }
       }
    }
    
}

    

 
